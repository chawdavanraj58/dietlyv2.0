function morn()
{
 	document.getElementById("breakfasttable").innerHTML = "";
	var arr = [ "Egg :","Juice :","Salad :"];
	
		var tabcr = document.getElementById("breakfasttable");
		var tabrow = document.createElement("tr");
		for(var j = 0; j < arr.length; j++ )
		{
			var tabdata = document.createElement("td");
			var inp = document.createElement("input");
			var txt= document.createTextNode(arr[j]);
			if(j == 0)
			{
				inp.setAttribute("type","number");
				inp.value="5";
				inp.setAttribute("name","begg");
			}
			
			if(j == 1)
			{
				inp.setAttribute("type","number");
				inp.value="500";
				inp.setAttribute("name","bjuice");
			}
			
			if(j == 2)
			{
				inp.setAttribute("type","number");
				inp.value="500";
				inp.setAttribute("name","bsalad");
			}
			tabdata.appendChild(txt);
			tabdata.appendChild(inp);
			tabrow.appendChild(tabdata);

		}
				tabcr.appendChild(tabrow);

}



